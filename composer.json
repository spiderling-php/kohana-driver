{
    "name": "spiderling-php/kohana-driver",
    "description": "Kohana crawler for spiderling",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ivan Kerin",
            "email": "ikerin@gmail.com",
            "role": "Author"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ivank/psr7"
        },
        {
            "type": "vcs",
            "url": "https://github.com/spiderling-php/crawler"
        }
    ],
    "require": {
        "spiderling-php/crawler": "dev-master",
        "spiderling-php/spiderling": "dev-master",
        "guzzlehttp/psr7": "dev-server-request as 1.3"
    },
    "require-dev": {
        "openbuildings/kohana-test-bootsrap": "~0.1.2"
    },
    "autoload": {
        "psr-4": {
            "SP\\KohanaDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SP\\KohanaDriver\\Test\\": "tests/src/"
        }
    }
}
